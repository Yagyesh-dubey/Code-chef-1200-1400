# include<bits/stdc++.h>
using namespace std;

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        set<string>set;
        
        for(int i=0;i<s.size()-1;i++)
        {
            set.insert(s.substr(i,2));
        }
        cout<<set.size()<<endl;
    }
    
    
    return 0;
}
