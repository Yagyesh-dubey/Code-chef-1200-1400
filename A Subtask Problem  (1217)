#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int m;
        cin>>m;
        int k;
        cin>>k;
        int a[n];
        int count=0;int count1=0;
        for(int i=0;i<n;i++){
            cin>>a[i];
        }
         for(int i=0;i<n;i++){
         
	        if(i<m)
	        {
	            if( a[i]==1 ) count++;
	        }
	        else
	        {
	            if( a[i]==1 ) count1++;
	        }
	    }
	     if(count==m)
	    {
	        if( (count + count1 )==n  ) cout<<"100"<<endl;
	        else cout<<k<<endl;
	    }
	    else cout<<"0"<<endl;
	    
            
            
        
         
         
    }
	return 0;
}
